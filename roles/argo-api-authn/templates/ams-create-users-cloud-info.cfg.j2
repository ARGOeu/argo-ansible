[AMS]

# under which ams project, the users will be created
ams_project: {{ams_cloudinfo_script.project}}

# goc db url to pull user data
goc_db_host: {{ams_cloudinfo_script.goc_db_host}}

# service types refers to the different service types that will we should keep from the xml and assign them to the respective ams topic
service-types: {{ams_cloudinfo_script.service_types}}

# cert to access gocdb
cert: {{ams_cloudinfo_script.cert}}
cert_key: {{ams_cloudinfo_script.cert_key}}

# ams use role
users_role: {{ams_cloudinfo_script.users_role}}

# ams consumer
ams_consumer: {{ams_cloudinfo_script.consumer}}

# token to access ams
ams_token: {{ams_cloudinfo_script.token}}

# ams url
ams_host: {{ams_cloudinfo_script.host}}

# ams user email
ams_email: {{ams_cloudinfo_script.email}}

[AUTHN]

# token to access authn
authn_token: {{authn_cloudinfo_script.token}}

# authn url to create bindings
authn_host: {{authn_cloudinfo_script.host}}

# service's uuid where bindings will belong
service_uuid: {{authn_cloudinfo_script.service_uuid}}

# service's host where bindings will belong
service_host: {{authn_cloudinfo_script.service_host}}

[LOGS]
syslog_socket: {{logs.syslog_socket}}
